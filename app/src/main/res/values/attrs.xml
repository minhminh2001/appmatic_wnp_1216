<resources>
    <style name="dialogAnim" parent="android:Animation" mce_bogus="1">
        <item name="android:windowEnterAnimation">@anim/dialog_enter_anim</item>
        <item name="android:windowExitAnimation">@anim/dialog_exit_anim</item>
    </style>

    <declare-styleable name="swipelistview">
        <attr name="inScroll">
            <enum name="yes" value="1"></enum>
            <enum name="no" value="0"></enum>
        </attr>

    </declare-styleable>


    <declare-styleable name="SwipeLayout">
        <attr name="drag_edge">
            <flag name="left" value="1" />
            <flag name="right" value="2" />
            <flag name="top" value="4" />
            <flag name="bottom" value="8" />
        </attr>
        <attr name="leftEdgeSwipeOffset" format="dimension" />
        <attr name="rightEdgeSwipeOffset" format="dimension" />
        <attr name="topEdgeSwipeOffset" format="dimension" />
        <attr name="bottomEdgeSwipeOffset" format="dimension" />
        <attr name="show_mode" format="enum">
            <enum name="lay_down" value="0" />
            <enum name="pull_out" value="1" />
        </attr>
        <attr name="clickToClose" format="boolean" />
    </declare-styleable>


    <declare-styleable name="CustomTextStyle">
        <attr name="textFont">
            <enum name="openSansSemibold" value="19" />
            <enum name="openSansRegular" value="18" />
            <enum name="openSansBold" value="8" />
            <enum name="arvoBold" value="23" />
            <!--<enum name="arvoBoldItalic" value="24" />-->
            <!--<enum name="arvoItalic" value="25" />-->
            <enum name="arvoRegular" value="26" />
            <enum name="ptSansBold" value="27" />
            <enum name="LatoRegular" value="28" />
            <enum name="LatoBold" value="29" />
            <enum name="LatoMedium" value="30" />
            <!--<enum name="ptSansCaptionBold" value="28" />-->
            <!--<enum name="ptSansCaptionItalic" value="29" />-->
            <!--<enum name="ptSansCaptionRegular" value="30" />-->
            <!--<enum name="ptSansItalic" value="31" />-->
            <!--<enum name="ptSansNarrowBold" value="32" />-->
            <!--<enum name="ptSansNarrowRegular" value="33" />-->
            <enum name="ptSansRegular" value="34" />

        </attr>
    </declare-styleable>
    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomCircularImageView">
        <attr name="border" format="boolean"></attr>
        <attr name="border_width" format="dimension"></attr>
        <attr name="border_color" format="color"></attr>
        <attr name="shadow" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="Theme">
        <attr name="circularImageViewStyle" format="reference"></attr>
    </declare-styleable>

    <declare-styleable name="CustomViewPagerStyle">
        <attr name="isStopHorizontalScroll" format="boolean" />
        <attr name="isDisallowParentTouch" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ToolBar">
        <attr name="myHint" format="string"/>
        <attr name="notNull" format="boolean"/>
        <attr name="isPassword" format="boolean"/>
        <attr name="isNumber" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="EditClear">
        <attr name="myClearHint" format="string"/>

    </declare-styleable>


    <string-array name="phoneNumber">
        <item>+60</item>
        <item>+80</item>
        <item>+86</item>
    </string-array>



    <declare-styleable name="CircularProgressView">
        <attr name="cpv_progress" format="float" />
        <attr name="cpv_maxProgress" format="float" />
        <attr name="cpv_animDuration" format="integer" />
        <attr name="cpv_animSwoopDuration" format="integer" />
        <attr name="cpv_animSyncDuration" format="integer" />
        <attr name="cpv_color" format="color"/>
        <attr name="cpv_thickness" format="dimension"/>
        <attr name="cpv_indeterminate" format="boolean" />
        <attr name="cpv_animAutostart" format="boolean" />
        <attr name="cpv_animSteps" format="integer" />
        <attr name="cpv_startAngle" format="float" />
    </declare-styleable>
    <declare-styleable name="MultiSwitchButton">
        <attr name="ThumbDrawable" format="reference"/>
        <attr name="ThumbColor" format="color|reference"/>
        <attr name="ThumbMargin" format="dimension|reference"/>
        <attr name="ThumbMarginTop" format="dimension|reference"/>
        <attr name="ThumbMarginBottom" format="dimension|reference"/>
        <attr name="ThumbMarginLeft" format="dimension|reference"/>
        <attr name="ThumbMarginRight" format="dimension|reference"/>
        <attr name="ThumbWidth" format="dimension|reference"/>
        <attr name="ThumbHeight" format="dimension|reference"/>
        <attr name="ThumbRadius" format="dimension|reference"/>
        <attr name="BackRadius" format="dimension|reference"/>
        <attr name="BackDrawable" format="reference"/>
        <attr name="BackColor" format="color|reference"/>
        <attr name="FadeBack" format="boolean"/>
        <attr name="BackMeasureRatio" format="float"/>
        <attr name="AnimationDuration" format="integer"/>
        <attr name="TintColor" format="color|reference"/>
        <attr name="TextOn" format="string"/>
        <attr name="TextOff" format="string"/>
        <attr name="TextMarginH" format="dimension"/>
        <attr name="AutoAdjustTextPosition" format="boolean"/>
    </declare-styleable>
    <!-- CustomCheckBxo-->
    <declare-styleable name="CustomCheckBox">
        <!-- 动画持续时间-->
        <attr name="duration" format="integer"/>
        <!-- 复选框的边框颜色-->
        <attr name="stroke_width" format="dimension"/>
        <!-- 标号颜色-->
        <attr name="color_tick" format="color"/>
        <!-- 选中时复选框的颜色-->
        <attr name="color_checked" format="color"/>
        <!-- 未选中是复选框的颜色-->
        <attr name="color_unchecked" format="color"/>
        <!-- 未选中时复选框边框的颜色-->
        <attr name="color_unchecked_stroke" format="color"/>
    </declare-styleable>
    <declare-styleable name="ParallaxScroll">
        <attr name="parallax_factor" format="float" />
        <attr name="alpha_factor" format="float" />
        <attr name="inner_parallax_factor" format="float" />
        <attr name="parallax_views_num" format="integer" />
        <attr name="circular_parallax" format="boolean" />
    </declare-styleable>

</resources>